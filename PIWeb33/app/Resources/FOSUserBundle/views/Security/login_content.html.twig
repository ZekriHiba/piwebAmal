{% trans_default_domain 'FOSUserBundle' %}

{% if error %}
    <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
{% endif %}


<section id="comingsoon" class="fullscreen paralbackground parallax" style="background-image:url({{ asset('Client/upload/xparallax_01.jpg.pagespeed.ic.bbBOmU960x.jpg') }})" data-img-width="1922" data-img-height="795" data-diff="100">
    <div class="js-height-full">
        <div class="home-text-wrapper container">
            <div class="home-message">


                <div class="newsletter">
                    <form class="form-inline" action="{{ path("fos_user_security_check") }}" method="post">
                        {% if csrf_token %}
                            <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                        {% endif %}
                        <div class="form-group">
                            <label style="color: white" for="username">{{ 'security.login.username'|trans }}</label>
                            <br>
                            <input class="form-control"  type="text" id="username" name="_username" value="{{ last_username }}" required="required" autocomplete="username" />
                            <br><br>
                            <label style="color: white" for="password">{{ 'security.login.password'|trans }}</label>
                            <br>
                            <input class="form-control"  type="password" id="password" name="_password" required="required" autocomplete="current-password" />
                            <br><br>
                            <a style="color: #7aa6da" href="{{ path("fos_user_resetting_request") }}">Mot de passe perdu?</a>
                            <br><br>
                            <hr style="color: white">
                            <br>
                            <label style="color: white">Vous n'avez pas encore de compte ? </label>
                           <a style="color: #7aa6da" href="{{ path("fos_user_registration_register") }}">Cr√©ez-en un !</a>


                            <br><br>
                            <input class="btn btn-normal btn-primary" type="submit" id="_submit" name="_submit" value="{{ 'security.login.submit'|trans }}"  />

                        </div>
                    </form>

                </div>
            </div>
        </div>
    </div>
</section>
